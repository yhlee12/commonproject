        }
        #custom_btn_search {
            /*width: 80px;
            height: 32px;
            line-height: 32px;
            color: #ffffff;
            background: #115C5E;
            text-align: center;*/
            cursor:pointer;
        }
        #custom_btn_reset {
            /*width: 80px;
            text-align: center;
            height: 32px;
            line-height: 32px;
            border: 1px solid #E2E2E3;*/
