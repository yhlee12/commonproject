}

/*sns 이미지*/
.sns_img {
    position: fixed; 
    top: 300px;
    right: 50px;
    z-index:999;
}

.sns_img_wrap {
    width:50px;
    height:50px;
    margin-bottom: 10px;
    border-radius:50%;
}

.sns_img_wrap > img {
    width: 100%;
    height:100%;
    object-fit:cover;
    cursor: pointer;
}

/*customer 이미지*/
.customer_img {
    position: fixed;
    width: 80px;
    height: 80px;
    bottom:120px;
    right:100px;
    z-index:999;
    cursor:pointer;
}

.customer_img > customer_img_wrap {
    width: 100%;
    height: 100%;
}

.customer_img > customer_img_wrap > img {
    width: 100%;
    height: 100%;
    object-fit:cover;
}

.customer_img_txt {
    position:relative;
    right: 20px;
    text-align: center;
    background-color: white;
    border-radius: 20px;
    width: 120px;
    padding: 5px 0px;
}


