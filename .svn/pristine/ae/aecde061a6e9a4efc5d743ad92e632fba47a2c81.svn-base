    height:350px;
}

/*magazine wrap*/
.magazine_wrap_inner {
    max-width: 1300px;
    margin: auto;
    padding: 5rem 0;
}

.magazine_txt {
    text-align: center;
    padding-bottom: 2rem;
}

.magazine_content_wrap_inner {
    max-width: 1300px;
    margin: auto;
    display: flex;
    justify-content: center;
    column-gap: 1rem;
}

.magazine_content_wrap_inner > .content {
    width: 400px;
