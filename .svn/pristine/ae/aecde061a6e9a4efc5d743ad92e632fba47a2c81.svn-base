body, html {
    min-width: 1400px;
    position: relative;
}

/*sns 이미지*/
.sns_img {
    position: fixed; 
    top: 300px;
    right: 50px;
    z-index:999;
}

.sns_img_wrap {
    width:50px;
    height:50px;
    margin-bottom: 10px;
    border-radius:50%;
}

.sns_img_wrap > img {
    width: 100%;
    height:100%;
    object-fit:cover;
}

/*customer 이미지*/
.customer_img {
    position: fixed;
    width: 80px;
    height: 80px;
    bottom:120px;
    right:100px;
    z-index:999;
}

.customer_img > customer_img_wrap {
    width: 100%;
    height: 100%;
}

.customer_img > customer_img_wrap > img {
    width: 100%;
    height: 100%;
    object-fit:cover;
}

.customer_img_txt {
    text-align: center;
}


/*헤더 css*/
.header_inner_wrap_top {
    background-color: #262626;
    color:fff;
}

.header_inner_wrap_top_txt {
    text-align: center;
    max-width: 1300px;
    padding: 0.5rem 0;
    margin: auto;
}

.header_inner_wrap_top_txt > span {
    color: white;
}

.header_inner_wrap_bottom {
    width: 100%;
}

.header_inner_wrap_bottom_member {
    max-width: 1300px;
    margin: auto;
    text-align:right;
    padding: 1rem 0;
}

.member-icon {
  
}

.header_inner_wrap_bottom_center {
     max-width: 1300px;
     margin: auto;
     display: flex;
     align-items: center;
     justify-content: space-between;
     padding: 2rem 0;
}

.logo {
    width: 12rem;
    height: 5rem;
}

.bottom_center_icon_wrap {
    display: flex;
    align-items: center;
    justify-content:center;
}

.bottom_center_icon_wrap > span {

    display: block;
    padding: 0 0.5rem;
}

.header_inner_wrap_bottom_menu_wrap_inner {
    max-width: 800px;
    margin:auto;
}

.header_inner_wrap_bottom_menu_wrap_inner > ul {
    display: grid;
    grid-template-columns: repeat(6, 1fr);
}

.header_inner_wrap_bottom_menu_wrap_inner > ul > li {
    display: block;
    padding: 0 1rem;
}
/*헤더 css*/

/*메인 contnet*/
.slider {
    width: 100%;
    overflow: hidden;
    position: relative;
}

.slider-container {
    display: flex;
    transition: transform 0.5s ease;
    padding: 2rem 0;
}

.slide {
    flex: 0 0 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.slide img {
    width: 100%;
    height: 100%;
    max-height: 100%;
    object-fit: cover;
}

.slider-controls {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 10px;
    right: 10px;
    display: flex;
    justify-content: space-between;
}

.slider-controls button {
    background: none;
    border: none;
    font-size: 8rem;
    color: #fff;
    cursor: pointer;
    padding: 5px;
}

/*best wrap*/
.best_wrap_inner {
    max-width: 1300px;
    margin: auto;
    padding: 5rem 0;
}

.best_txt {
    text-align: center;
}

.best_menu {
    max-width: 800px;
    margin: auto; 
    text-align: center;
    padding: 2rem 0;
}

.best_menu > ul {
    display: grid;
    grid-template-columns: repeat(5, 1fr);
    gap: 3rem;
}

.best_menu > ul > li {
    display: block;
    border-bottom: 2px solid;
    padding-bottom: 0.5rem;
}

.best_menu > ul > li:hover {
    color:#115c5e;
    border-color:#115c5e;
}

.best_content_wrap_inner {
    max-width: 1300px;
    margin: auto;
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    column-gap: 1rem;
}

.best_content_wrap_inner > .content {
    position: relative;
    border: 1px solid #707070;
    height: 300px;
}

.best_content_wrap_inner > .content > .best_txt {
    position: absolute;
    padding: 0.5rem 1rem;
    border-radius:10px;
    top: 10px;
    left: 10px;
}

/*suggection wrap*/
.suggection_wrap_inner {
    max-width: 1300px;
    margin: auto;
    padding: 5rem 0;
}

.suggection_txt {
    text-align: center;
    padding-bottom: 2rem;
}

.suggection_content_wrap_inner {
    max-width: 1300px;
    margin: auto;
    display: flex;
    /*grid-template-columns: repeat(4, 1fr);*/
    column-gap: 1rem;
}

.suggection_content_wrap_inner > .content {
    position: relative;
    border: 1px solid #707070;
    height: 300px;
    width: 25%;
}

.suggection_content_wrap_inner > .content > .suggection_txt {
    position: absolute;
    padding: 0.5rem 1rem;
    border-radius:10px;
    top: 10px;
    left: 10px;
}

.bottom_discount {
    display:flex;
}

.bottom_discount_first, 
.bottom_discount_last {
    border:1px solid #cacaca;
    padding: 6px;
}

/*business wrap*/
.business_wrap_inner_wrap {

}

.business {
    max-width: 1300px;
    margin: auto;
    display: flex;
    justify-content: space-between;
    padding: 50px 0;
}

.business_button_wrap {
    
}

.business_button_wrap_inner {
    display: flex;
}
    
.business_button_wrap_txt,
.business_button_wrap_arrow {
    height:30px;
}

.business_button_wrap_txt,
.business_button_wrap_arrow > span {
