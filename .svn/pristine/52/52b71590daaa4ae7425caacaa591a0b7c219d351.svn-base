    <script>
        $(document).ready(function () {
            $('#hover5').click(function () {

                $('#hover5').css('color', 'white');
                $('#hover5').css('border', '1px solid #115C5E');
                $('#hover5').css('background-color', '#115C5E');
                $('#hover6, #hover7').css('color', 'black');
                $('#hover6, #hover7').css('border', '1px solid #e2e2e2');
                $('#hover6, #hover7').css('background-color', '#fff');

                $('#hover6_wrap, #hover7_wrap').hide();
                $('#hover6_text_wrap, #hover7_text_wrap').hide();
                $('#hover5_text_wrap').css({
                    display: 'block'
                });
                $('#hover5_wrap').css({
                    display: 'block',
                    position: 'relative',
                    top: '20px',
                    opacity: 0
                }).animate({
                    top: '0px',
                    opacity: 1
                }, 500);

            });

            $('#hover6').click(function () {

                $('#hover6').css('color', 'white');
                $('#hover6').css('border', '1px solid #115C5E');
                $('#hover6').css('background-color', '#115C5E');
                $('#hover5, #hover7').css('color', 'black');
                $('#hover5, #hover7').css('border', '1px solid #e2e2e2');
                $('#hover5, #hover7').css('background-color', '#fff');

                $('#hover5_wrap, #hover7_wrap').hide();
                $('#hover5_text_wrap, #hover7_text_wrap').hide();
                $('#hover6_text_wrap').css({
                    display: 'block'
                });
                $('#hover6_wrap').css({
                    display: 'block',
                    position: 'relative',
                    top: '20px',
                    opacity: 0
                }).animate({
                    top: '0px',
                    opacity: 1
                }, 500);
            });

            $('#hover7').click(function () {

                $('#hover7').css('color', 'white');
                $('#hover7').css('border', '1px solid #115C5E');
                $('#hover7').css('background-color', '#115C5E');
                $('#hover6, #hover5').css('color', 'black');
                $('#hover6, #hover5').css('border', '1px solid #e2e2e2');
                $('#hover6, #hover5').css('background-color', '#fff');

                $('#hover5_wrap, #hover6_wrap').hide();
                $('#hover5_text_wrap, #hover6_text_wrap').hide();
                $('#hover7_text_wrap').css({
                    display: 'block'
                });
                $('#hover7_wrap').css({
                    display: 'block',
                    position: 'relative',
                    top: '20px',
                    opacity: 0
                }).animate({
                    top: '0px',
                    opacity: 1
                }, 500);
            });

            //리뷰 확인 창
            $("#review_search_button").click(function () {
                $(".review_search_wrap_inner").css({
                    "display": "block",
                    "z-index": "999"
                });

                $("<div>")
                    .addClass("overlay")
                    .css({
                        "position": "fixed",
                        "top": 0,
                        "left": 0,
                        "width": "100%",
                        "height": "100%",
                        "background-color": "rgba(0, 0, 0, 0.5)",
                        "z-index": "998"
                    })
                    .appendTo("body");
            });
            //확인 버튼
            $(".first_button").click(function () {
                window.location.href = "/Source/client/member/login/C_LOGIN_Page.aspx";
            });

            //취소 버튼
            $(".second_button").click(function () {
                $(".overlay").remove();
                $(".review_search_wrap_inner").css("display", "none");
            });

            //img review 확인 창
            $("#img_review_button").click(function () {
                $(".img_review_wrap_inner").css({
                    "display": "block",
                    "z-index": "999"
                });

                $("<div>")
                    .addClass("overlay")
                    .css({
                        "position": "fixed",
                        "top": 0,
                        "left": 0,
                        "width": "100%",
                        "height": "100%",
                        "background-color": "rgba(0, 0, 0, 0.5)",
                        "z-index": "998"
                    })
                    .appendTo("body");
            });
            //취소 버튼
            $(".img_wrap_clear > span").click(function () {
                $(".overlay").remove();
                $(".img_review_wrap_inner").css("display", "none");
            });

            //txt review 확인 창
            $("#txt_review_button").click(function () {
                $(".txt_review_wrap_inner").css({
                    "display": "block",
                    "z-index": "999"
                });

                $("<div>")
                    .addClass("overlay")
                    .css({
                        "position": "fixed",
                        "top": 0,
                        "left": 0,
                        "width": "100%",
                        "height": "100%",
                        "background-color": "rgba(0, 0, 0, 0.5)",
                        "z-index": "998"
                    })
                    .appendTo("body");
            });
            //취소 버튼
            $(".img_wrap_clear > span").click(function () {
                $(".overlay").remove();
                $(".txt_review_wrap_inner").css("display", "none");
            });
        });
    </script>

    <%--이미지 교체--%>
    <script>
        $(document).ready(function () {
