body,html {
    min-width:320px;
    overflow-x:hidden;
    -ms-overflow-style: none;
    font-size: 20px;
}

::-webkit-scrollbar {
  display: none;
}

a {
    color: black;
}

.m_header_wrap_inner {
    padding: 0.5rem 0;
    color:white;
    background-color:#262626;
    display:flex;
    align-items:center;
    justify-content:center;
}

.m_header_content_wrap_inner {
    display: flex;
    align-items: center;
    justify-content: space-around;
    padding: 1rem 0 15px;
}

.m_box {
    cursor: pointer;
}

.logo {
    width: 4.5rem;
    margin-left: 1.6rem;
}

.bottom_center_icon_wrap {
    display:flex;
    align-items:center;
    justify-content:center;
    gap: 0.3rem;
}

.bottom_center_icon_wrap > span {
    display:block;
}
.bottom_center_icon_wrap > a > span {
    display:block;
}

.m_icon {
    font-size: 1.2rem;
}

/*customer 이미지*/
.customer_img {
    position: fixed;
    width: 2.5rem;
    height: 2.5rem;
    bottom: 7rem;
    right: 2rem;
    z-index: 990;
    cursor:pointer;
}

.customer_img > customer_img_wrap {
    width: 100%;
    height: 100%;
}

.customer_img > customer_img_wrap > img {
    width: 100%;
    height: 100%;
    object-fit:cover;
}

/*sns 이미지*/
.sns_img {
    position: fixed; 
    top: 8rem;
    right: 0.6rem;
    z-index:990;
}

.sns_img_wrap {
    width: 1.6rem;
    height: 1.6rem;
    margin-bottom: 0.5rem;
    border-radius:50%;
}

.sns_img_wrap > img {
    width: 100%;
    height:100%;
    object-fit:cover;
    cursor: pointer;
}

.customer_img_txt {
    position: relative;
    right: 1.2rem;
    text-align: center;
    background-color: white;
    border-radius: 20px;
    /* width: 120px; */
    width: 5rem;
    line-height: 1.2;
    padding: 5px 0px;
    font-size: 0.6rem;
}

/*메인 최하단 메뉴*/
.m_bottom {
    position:fixed;
    bottom:0;
    width:100%;
    z-index:999;
}

.m_bottom_wrap_inner {
    display: flex;
    align-items: center;
    justify-content:space-around;
    background-color:#fff;
    padding:0.5rem 0.8rem;
}

.icon_menu {
    text-align: center;
    cursor:pointer;
}

.m_bottom_wrap_inner > .icon_menu:first-child > span {
    color:#115c5e;
}

.icon_menu > span {
    display:block;
    color:#BBBBBB;
}

.icon_menu > span:last-child {
    display:block;
}

.weight600 {
    font-weight: 600;
}

/*menu_hide_button 전체 메뉴 클릭시*/
.menu_hide {
  position: fixed;
  left: -100%;
  width: 100%;
  height:calc(100vh - 68px);
  z-index:999;
  background-color: #ffffff;
  transition: left 1s;
  top: 0;
  padding: 15px;
  overflow: scroll;
}

.menu_hide_wrap_inner {
    display:flex;
    align-items:center;
    justify-content:space-between;
}

.customer_menu {
    display:flex;
    align-items:center;
    justify-content:space-between;
    gap:0.7rem;
}

.customer_menu > .customer_menu_content {
    cursor:pointer;
}

.menu_hide_clear > span {
    display:block;
    cursor:pointer;
}

.my_menu {
    display:flex;
    align-items:center;
    justify-content:space-around;
    padding: 1.5rem 0;
    border-bottom: 1px solid #ccc;
}

.my_menu_content {
    text-align:center;
    cursor:pointer;
}

.my_menu_content > span {
    display:block;
    color:#828282;
}

.my_menu_content > span:first-child {
   font-size: 2rem;
}

.my_menu_content > span:last-child {
    display:block;
}

.hide_txt {
    padding-top:1rem;
    border-bottom: 1px solid #ccc;
}

.hide_txt_content {
    display:flex;
    align-items:center;
    justify-content:space-between;
    padding-bottom: 1rem;
}

.hide_txt_content > a > span:first-child {
    font-weight: 600;
}

.hide_txt_content > .sub_title {
    color:#828282;
}

/*모바일 메인 검색창*/
/*메인 검색창*/
.search {
  position: fixed;
  width:100%;
  right: -100%;
  z-index:999;
  transition: right 1s;
  top: 0;
  padding: 15px;
  overflow: scroll;
  background-color: #fff;
}

.search_wrap_inner {
  width: 100%;
  height:calc(100vh - 68px);
  margin: auto;
}

.search_clear { 
  text-align: end;
}

.search_clear > span {
    cursor:pointer;
    font-size:30px;
}

.search_main_title {
  color: #262626;
  font-weight: 600;
  text-align: center;
  margin-bottom: 2rem;
}

.title_search {
  border-bottom: 2px solid;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.title_search > input {
  border:none;
  width: 100%;
  padding-left:15px;
}

.title_search > span {
    cursor:pointer;
    font-size:30px;
}

.bottom_title > .bottom_main_title  {
  padding:1.5rem 0 1rem;
  color:#111111;
  font-weight: 500;
}

.bottom_main_title {
    position: relative;
}

.bottom_main_title > span {
    position: relative;
    z-index: 1;
    font-weight:600;
}

.bottom_main_title::after {
    content: '';
    position: absolute;
    left: 3.8rem;
    width: 7px;
    height: 7px;
    background-color: #115C5E;
    border-radius: 50%;
    z-index: 0;
}

.circle_title {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
}

.circle_title > .circle_title_content {
  width: 8rem;
  height: 2rem;
  border: 1px solid #cacaca;
  color: #cacaca;
  border-radius: 50px;
  padding-left: 1rem;
  cursor:pointer;
  margin-bottom: 0.5rem;
}

.circle_title > .circle_title_content > span {
  line-height: 2rem;
}

/*메인 팝업창*/
/*m_main fixed 팝업창*/
.popup_sldie {
    position:fixed;
    width:100%;
    /*height:11rem;*/
    bottom: 68px;
    z-index: 998;
    display:block;
}

.popup_sldie_wrap_inner {
    border-radius: 10px;
}

.popup_sldie_wrap_inner > .swiper-container {
    width: 100%;
    height:12rem;
    border-top-right-radius: 10px;
    border-top-left-radius: 10px;
}

.swiper-slide {
    border-top-right-radius: 20px;
    border-top-left-radius: 20px;
}

.swiper-slide > img {
    border-top-right-radius: 20px;
    border-top-left-radius: 20px;
}

.swiper-wrapper > .first_silde {
    background-color: #1d9d9a;
}

.swiper-wrapper > .second_silde {
    background-color: #fff;
}

.swiper-wrapper > .third_silde {
    background-color: #ffc609;
}

.popup_sldie_wrap_inner > .clear_under {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: end;
    background-color: #fff;
    padding: 0.4rem 0;
}

.popup_sldie_wrap_inner > .clear_under > span {
    display:block;
}

.clear_under > span:nth-child(2) {
    padding: 0 10px 0 5px;
    cursor: pointer;
}

/*overlay*/
.overlay {
    position:fixed;
    top:0;
    left:0;
    width:100%;
    height:100%;
    background-color:rgba(0, 0, 0, 0.5);
    z-index:997;
}

/*메인 배너 슬라이드*/
.slide {
    display: flex;
    flex-wrap: nowrap;
    overflow: hidden;
    position: relative;
    width: 100%;
    user-select: none;
}
.slide_item {
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    left: 0px;
    width: 100%;
    min-height:19rem;
    flex-shrink: 0;
    transition: left 0.15s;
}

.slide_item > img {
    width: 100%;
    height:100%;
    object-fit: cover;
}

.slide_pagination {
    display: flex;
    gap: 5px;
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    z-index: 990;
}

.slide_pagination > li {
    color: #4a8b8d;
    cursor: pointer;
    font-size: 25px;
}
.slide_pagination > li.active {
    color: #115c5e;
}

.slide_item_duplicate {
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    left: 0px;
    width: 100%;
    flex-shrink: 0;
    transition: left 0.15s;
}

