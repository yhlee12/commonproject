html, body, form {
    width: 100%;
    height: 100%;
}

#wrap {
    width: 100%;
    height: 100%;
    min-width: 1366px;
    min-height: 768px;
    position: relative;
    overflow: auto;
}

#main-panel {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    padding-left: 250px;
    background-color: var(--mono_back);
}
/*icon field*/
#icon-field {
    width: 100%;
    height: 200px;
    position: relative;
    border-bottom: 1px solid var(--mono_dea);
}

#icon-inner {
    top: 50%;
    left: 50%;
    position: absolute;
    transform: translate(-50%,-50%);
    max-width: 90%;
}

.drkid-icon-admin {
    width: 150px;
    margin: auto;
}

    .drkid-icon-admin > img {
        width: 100%;
        height: 100%;
        object-fit: cover;
    }

.go-client-home {
    margin-top: 10px;
    width: 185px;
    height: 30px;
    line-height: 28px;
    color: var(--mono_dea);
    border: 1px solid var(--mono_dea);
    border-radius: 30px;
    text-align: center;
    cursor: pointer;
}
/*search field*/
#search-field {
    width: 100%;
    height: 220px;
    padding: 20px 30px;
}

#search-field-inner {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100%;
    height: 100%;
    justify-content: space-between;
}

.search-box-warper {
    width: 100%;
}

.search-field-input {
    width: 100%;
    border-bottom: 1px solid var(--mono_dea);
    padding-right: 35px;
    position: relative;
}

    .search-field-input input {
        width: 100%;
        outline: none;
        border: none;
        background-color: transparent;
        color: var(--mono_dea);
        box-sizing: border-box;
        height: 30px;
        line-height: 30px;
    }

    .search-field-input > .search-icon {
        position: absolute;
        top: 50%;
        right: 5px;
        transform: translate(0,-50%);
        cursor: pointer;
    }

        .search-field-input > .search-icon:after {
            font-size: 24px;
            color: var(--mono_dea);
        }

.search-btn-div {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
}

    .search-btn-div > .drkid-btn-sub {
        background-color: transparent;
        color: var(--mono_dea);
        border: 1px solid var(--mono_dea);
        min-width: calc(50% - 10px);
        min-height: 30px;
        height: 30px;
    }
/*left Panel*/
#left-panel {
    width: 250px;
    height: 100%;
    background-color: var(--main_color);
    position: absolute;
    top: 0;
    left: 0;
}
/*Menu*/

#menu-field {
    width: 100%;
    height: calc(100% - 420px);
    background-color: var(--sub_color);
    padding-top: 10px;
    overflow: auto;
}

.menu-main-ul {
    width: 100%;
}

.menu-main-li {
    width: 100%;
    box-sizing: border-box;
    background-color: var(--sub_color);
    cursor: pointer;
}

    .menu-main-li > .menu-main-div {
        color: white;
        padding: 19px 30px;
        position: relative;
        padding-right: calc(30px + 8px);
    }

.menu-main-div > .menu-arrow-div {
    position: absolute;
    top: 50%;
    right: 38px;
    width: 8px;
    height: 8px;
    line-height: 8px;
    transform: translate(0,-50%);
}

.menu-main-li > .menu-under-bar {
    width: 85%;
    margin: auto;
    height: 1px;
    background-color: var(--mono_dea);
    border: none;
}

.menu-sub-ul {
    width: 100%;
    padding: 10px 0;
    background-color: var(--main_color);
    display: none;
}

    .menu-sub-ul > li {
        padding: 8px 30px;
        color: var(--mono_dea);
    }
/*Top*/
#main-top {
    width: 100%;
    height: 85px;
    border-bottom: 1px solid var(--mono_line);
    position: relative;
    display: flex;
    align-items: center;
    justify-content: flex-start;
}

.top-ring {
    width: 30px;
    height: 30px;
    padding: 0 28px;
    box-sizing: content-box;
}

    .top-ring > .top-ring-span {
        font-size: 30px;
        text-align: center;
        color: var(--main_color);
        width: 30px;
        height: 30px;
    }

.top-block {
    height: 60px;
    border: 1px solid var(--mono_line);
    padding: 0 15px;
    background-color: white;
    border-radius: 6px;
    margin-right: 25px;
}

.top-block-inner {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    height: 100%;
}

    .top-block-inner span {
        font-weight: 600;
        cursor: pointer;
    }

.frist-block {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 15px;
}

.block-count {
    margin-left: 8px;
}

.sub-block {
    height: calc(100% - 20px);
    border-radius: 6px;
    border: 1px solid var(--mono_line);
    background-color: var(--mono_back);
    padding: 0 17px;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.sub-inner-block {
    margin-left: 15px;
}

    .sub-inner-block:first-child {
        margin-left: 0;
    }

.sub-font-weight {
    font-weight: 400 !important;
}
