    <script>
        $(document).ready(function () {

            var activeDropdown = null;

            $('#menu1').mouseenter(function () {
                showDropdown('#dropdown1');
            });

            $('#menu2').mouseenter(function () {
                showDropdown('#dropdown2');
            });

            $('#menu3').mouseenter(function () {
                showDropdown('#dropdown3');
            });

            $(document).on('click', function (event) {
                var target = $(event.target);
                if (!target.closest('.dropdown').length) {
                    hideDropdown(activeDropdown);
                }
            });

            function showDropdown(dropdownId) {
                if (activeDropdown !== dropdownId) {
                    hideDropdown(activeDropdown);
                    $(dropdownId).css('display', 'block');
                    activeDropdown = dropdownId;
                }
            }

            function hideDropdown(dropdownId) {
                if (dropdownId !== null) {
                    $(dropdownId).css('display', 'none');
                    activeDropdown = null;
                }
            }

            const prevBtn = $("#prevBtn");
            const nextBtn = $("#nextBtn");
            const slides = $(".slide");
            const slideWidth = slides.width(); // Assuming all slides have the same width
            let currentSlide = 0;

            prevBtn.on("click", function () {
                currentSlide = (currentSlide - 1 + slides.length) % slides.length;
                updateSlide("right");
            });

            nextBtn.on("click", function () {
                currentSlide = (currentSlide + 1) % slides.length;
                updateSlide("left");
            });

            function updateSlide(direction) {
                const offset = direction === "left" ? -slideWidth : slideWidth;
                slides.eq(currentSlide).animate({ marginLeft: offset }, 600, function () {
                    slides.css("margin-left", 0);
                    slides.eq(currentSlide).appendTo(slides.parent());
                });
            }

            updateSlide();


            $('[data-serialscrolling]').serialscrolling();
        });
    </script>

    <script>
        $(document).ready(function () {
            $('#menu2, #menu3').mouseenter(function () {
                var menuId = $(this).attr('id');
                var dropdownId = '#dropdown' + menuId.substr(4); 

                var menuLeft = $(this).offset().left;
                $(dropdownId).css('left', menuLeft + 'px');
            });
        });
    </script>

