    <script>
        $(document).ready(function () {
            // Variables
            var slideWrapper = $(".you_wrap_inner");
            var slideContent = $(".you_content");
            var slides = $(".you_content > div");
            var slideWidth = slideWrapper.width();
            var currentSlide = 0;

            // Set initial position of slides
            slides.css("width", slideWidth);

            // Handle previous button click
            $(".swiper-button-prve").click(function () {
                if (currentSlide > 0) {
                    currentSlide--;
                    slideWrapper.animate({ left: -slideWidth * currentSlide });
                }
            });

            // Handle next button click
            $(".swiper-button-next").click(function () {
                if (currentSlide < slides.length - 1) {
                    currentSlide++;
                    slideWrapper.animate({ left: -slideWidth * currentSlide });
                }
            });
        });
    </script>
    
