    <%--show hide button--%>
    <script>
        $(document).ready(function () {
            var isAnimating = {};

            $(".hide_button").click(function () {
                var targetId = $(this).data("target");
                if (!isAnimating[targetId]) {
                    isAnimating[targetId] = true;
                    $("#hide_button_wrap_" + targetId).slideUp(500, function () {
                        $(this).addClass("hidden");
                        $(".product_name_wrap_title#" + targetId).addClass("rotate"); // Remove the rotate class
                        isAnimating[targetId] = false;
                    });
                }
            });

            $(".product_name_wrap_title").click(function () {
                var targetId = this.id;
                if (!isAnimating[targetId]) {
                    isAnimating[targetId] = true;
                    $("#hide_button_wrap_" + targetId).slideToggle(500, function () {
                        $(this).toggleClass("hidden");
                        $(".product_name_wrap_title#" + targetId).toggleClass("rotate"); // Toggle the rotate class
                        // Set the transition time dynamically to match slideUp time
                        $(".product_name_wrap_title#" + targetId + " .material-icons").css("transition", "transform 0.5s ease");
                        isAnimating[targetId] = false;
                    });
                }
            });
        });
    </script>

    <%--옵션 기능--%>
    <script>
        $(document).ready(function () {
            var maxOptions = 3;
            var currentOptions = 1;

            <%--옵션추가--%>
            $('#OptionAddButton').on('click', function () {
                if (currentOptions < maxOptions) {
                    var existingOptionBox = $('.option_box_wrap').first().clone();
                    existingOptionBox.css('border-top', '1px solid #E2E2E2');

                    var deleteIcon = $('<span class="delete-icon">옵션 삭제하기</span>');
                    existingOptionBox.append(deleteIcon);

                    deleteIcon.on('click', function () {
                        existingOptionBox.remove();
                        currentOptions--;
                    });

                    $('#OptionAddWrap').append(existingOptionBox);
                    currentOptions++;
                }
            });

            //옵션 설정 유무 버튼
            $('#optionSetButton').click(function () {
                $('.option_total').css('display', 'block');
            });

            $('#optionCloseButton').click(function () {
                $('.option_total').css('display', 'none');
            });            
        });
    </script>

    <script>
        $(document).ready(function () {
            // Function to show the active content
            function showActiveContent(selectedOption) {
                $(".drkid-select-ul li").removeClass("active_one active_two active_three"); // Remove active classes from all li elements

                // Add the corresponding active class to the selected li
                if (selectedOption === 0) {
                    $(".drkid-select-ul li:eq(0)").addClass("active_one");
                    $("#option_price_one").show(); // Show option_price_one
                    $("#option_price_two, #option_price_three").hide(); // Hide option_price_two and option_price_three
                } else if (selectedOption === 1) {
                    $(".drkid-select-ul li:eq(1)").addClass("active_two");
                    $("#option_price_two").show(); // Show option_price_two
                    $("#option_price_one, #option_price_three").hide(); // Hide option_price_one and option_price_three
                } else if (selectedOption === 2) {
                    $(".drkid-select-ul li:eq(2)").addClass("active_three");
                    $("#option_price_three").show(); // Show option_price_three
                    $("#option_price_one, #option_price_two").hide(); // Hide option_price_one and option_price_two
                }
            }

            // Attach click event handler to the li elements
            $(".drkid-select-ul li").click(function () {
                var selectedOption = $(this).index(); // Get the index of the clicked li
                showActiveContent(selectedOption);
            });

            // Initialize active content on page load
            showActiveContent(0); // Show the content for the first li by default
        });
    </script>

