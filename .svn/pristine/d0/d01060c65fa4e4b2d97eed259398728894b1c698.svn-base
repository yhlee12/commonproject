osition:relative;padding-right:calc(30px + 8px)}.menu-main-div>.menu-arrow-div{position:absolute;top:50%;right:38px;width:8px;height:8px;line-height:8px;transform:translate(0,-50%)}.menu-main-li>.menu-under-bar{width:85%;margin:auto;height:1px;background-color:var(--mono_dea);border:none}.menu-sub-ul{width:100%;padding:10px 0;background-color:var(--main_color);display:none}.menu-sub-ul>li{padding:8px 30px;color:var(--mono_dea)}#main-top{width:100%;height:85px;border-bottom:1px solid var(--mono_line);position:relative;display:flex;align-items:center;justify-content:flex-start}.top-ring{width:30px;height:30px;padding:0 28px;box-sizing:content-box}.top-ring>.top-ring-span{font-size:30px;text-align:center;color:var(--main_color);width:30px;height:30px}.top-block{height:60px;border:1px solid var(--mono_line);padding:0 15px;background-color:#fff;border-radius:6px;margin-right:25px}.top-block-inner{display:flex;justify-content:flex-start;align-items:center;height:100%}.top-block-inner span{font-weight:600;cursor:pointer}.frist-block{display:flex;justify-content:center;align-items:center;margin-right:15px}.block-count{margin-left:8px}.sub-block{height:calc(100% - 20px);border-radius:6px;border:1px solid var(--mono_line);background-color:var(--mono_back);padding:0 17px;display:flex;justify-content:flex-start;align-items:center}.sub-inner-block{margin-left:15px}.sub-inner-block:first-child{margin-left:0}.sub-font-weight{font-weight:400!important}