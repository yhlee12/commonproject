        .BackGround_Container{
            margin : 10px;
            background-color : white;
        }
        .Title-wrap{
            background-color: white;
            margin: 12px 12px 0 12px;
            border-bottom: 1px solid rgb(219, 221, 226);
        }
        .Main_wrap{
            background-color : white;
            margin: 0 12px;
            padding : 28px 0 ;
            border-bottom: 1px solid rgb(219, 221, 226);
        }
        .sub-wrap{
            font-weight : bold;
        }
        .Main_wrap{
            padding : 20px 30px;
        }
        .Today_Detail_BTN, .Today_Delete_BTN{
